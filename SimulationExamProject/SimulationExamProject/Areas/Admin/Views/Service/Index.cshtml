@model IEnumerable<Service>;


<a type="button" class="btn btn-success mb-3" asp-area="Admin" asp-controller="Service" asp-action="Create" >Create service</a>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Title</th>
            <th scope="col">Description</th>
            <th scope="col">IsActive</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Service item in Model){
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Title</td>
                <td>@item.Description</td>
                <td>@item.IsActive</td>
                <td><a class="btn btn-primary" asp-area="Admin" asp-controller="Service" asp-action="Update" asp-route-id="@item.Id">Update</a></td>
                <td><a class="btn btn-danger" asp-area="Admin" asp-controller="Service" asp-action="Delete" asp-route-id="@item.Id">Delete</a></td>
            </tr>
        }
    
       
    </tbody>
</table>
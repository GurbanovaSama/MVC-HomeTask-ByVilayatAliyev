@model IEnumerable<Order>;


<a type="button" class="btn btn-success mb-3" asp-area="Admin" asp-controller="Order" asp-action="Create">Create order</a>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">ClientName</th>
            <th scope="col">ClientSurname</th>
            <th scope="col">ClientPhoneNumber</th>
            <th scope="col">ClientEmail</th>
            <th scope="col">ServiceId</th>
            <th scope="col">Problem</th>
            <th scope="col">IsAcive</th>
  

        </tr>
    </thead>
    <tbody>
        @foreach (Order item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.ClientName</td>
                <td>@item.ClientSurname</td>
                <td>@item.ClientPhoneNumber</td>
                <td>@item.ClientEmail</td>
                <td>@item.ServiceId</td>
                <td>@item.Problem</td>
                <td>@item.IsActive</td>

                <td><a class="btn btn-primary" asp-area="Admin" asp-controller="Order" asp-action="Update" asp-route-itemid="@item.Id">Update</a></td>
                <td><a class="btn btn-danger" asp-area="Admin" asp-controller="Order" asp-action="Delete" asp-route-id="@item.Id">Delete</a></td>
            </tr>
        }


    </tbody>
</table>